#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/code-analyzer-exten-backend/node_modules/.pnpm/nginx@1.0.8/node_modules/nginx/node_modules:/mnt/d/code-analyzer-exten-backend/node_modules/.pnpm/nginx@1.0.8/node_modules:/mnt/d/code-analyzer-exten-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/code-analyzer-exten-backend/node_modules/.pnpm/nginx@1.0.8/node_modules/nginx/node_modules:/mnt/d/code-analyzer-exten-backend/node_modules/.pnpm/nginx@1.0.8/node_modules:/mnt/d/code-analyzer-exten-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nginx/index.js" "$@"
else
  exec node  "$basedir/../nginx/index.js" "$@"
fi
