#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/ComplexityAnalyzersExten/Backend/node_modules/.pnpm/jq@1.7.2/node_modules/jq/bin/node_modules:/mnt/d/ComplexityAnalyzersExten/Backend/node_modules/.pnpm/jq@1.7.2/node_modules/jq/node_modules:/mnt/d/ComplexityAnalyzersExten/Backend/node_modules/.pnpm/jq@1.7.2/node_modules:/mnt/d/ComplexityAnalyzersExten/Backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/ComplexityAnalyzersExten/Backend/node_modules/.pnpm/jq@1.7.2/node_modules/jq/bin/node_modules:/mnt/d/ComplexityAnalyzersExten/Backend/node_modules/.pnpm/jq@1.7.2/node_modules/jq/node_modules:/mnt/d/ComplexityAnalyzersExten/Backend/node_modules/.pnpm/jq@1.7.2/node_modules:/mnt/d/ComplexityAnalyzersExten/Backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jq/bin/jq" "$@"
else
  exec node  "$basedir/../jq/bin/jq" "$@"
fi
