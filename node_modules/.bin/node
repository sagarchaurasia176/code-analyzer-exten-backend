#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Backend/node_modules/.pnpm/node@23.9.0/node_modules/node/bin/node_modules:/mnt/d/Backend/node_modules/.pnpm/node@23.9.0/node_modules/node/node_modules:/mnt/d/Backend/node_modules/.pnpm/node@23.9.0/node_modules:/mnt/d/Backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Backend/node_modules/.pnpm/node@23.9.0/node_modules/node/bin/node_modules:/mnt/d/Backend/node_modules/.pnpm/node@23.9.0/node_modules/node/node_modules:/mnt/d/Backend/node_modules/.pnpm/node@23.9.0/node_modules:/mnt/d/Backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../node/bin/node.exe"   "$@"
exit $?
